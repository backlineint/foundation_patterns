{% embed "@marketing/footer/marketing-site-footer.twig" %}

{% block social_menu %}
  {% for link in social_links %}
    {% include "@global-patterns/social-link/social-link.twig"
      with {
        "href": link.href,
        "icon_class": link.icon_class
      }
    %}
  {% endfor %}
{% endblock %}

{% block bottom_menu %}
  {% for link in bottom_links %}
    {% include "@global-patterns/menu-link/menu-link.twig"
      with {
        "link": link.link
      }
    %}
  {% endfor %}
{% endblock %}

{% endembed %}